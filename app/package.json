{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "description": "Mazel's Personnal WebSite",
  "author": "Mazel <mazuel.loic@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
